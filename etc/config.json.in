{
        "port": 2020,
        "bunyan": {
                "level": "info",
                "syslog": {
                        "facility": "local0",
                        "type": "udp"
                }
        },
        "manatee": {
                "path": "/manatee/sdc/election",
                "pg": {
                        "connectTimeout": 3000,
                        "maxConnections": 15,
                        "maxIdleTime": 120000,
