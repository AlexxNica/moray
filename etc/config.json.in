                        "queryTimeout": 30000
                },
                "zk": {
                        "servers": [ {
                                "host": "ZK_IP",
                                "port": 2181
                        } ],
                        "timeout": 60000
                }
        },
        "cache": {
                "size": 1000,
                "expiry": 60
        }
}
